            var mediaURLin, tag = document.createElement('script'); tag.src = "https://www.youtube.com/iframe_api"
            firstScriptTag = document.getElementsByTagName('script')[0]; firstScriptTag.parentNode.insertBefore(tag, firstScriptTag)
            document.getElementById("mediaplayer").addEventListener("keyup", function(event) {
                if (event.keyCode === 13) { event.preventDefault(); document.getElementById("mediain").click()}}); //event listener for enter key
                
            function mediainput(){ 
                mediaURLin = document.getElementById("mediaplayer").value //reads text input
                inprompt = inprompt + "#read: " + mediaURLin; println() // updates the command prompt
                mediaURLin = mediaURLin.replace("https://www.youtube.com/", "").replace("https://youtu.be/","").replace("playlist?list=","").replace("watch?v=","").replace(" ","") //strips down to the playlist ID + video id
                inprompt = inprompt + "^" + mediaURLin.length + ", id:" + mediaURLin;println(); // posts the stripped down string into the command prompt
                videoclear(); playlistlaunch(); //required to clear the current video in the player, then plays the playlist + video ID.
                var index = 0, videoin = ""; // clears the playlist index and empties the video ID string.
                if(mediaURLin.length < 20){ //check if the ID has a playlist ID, runs if it doesn't.
                    if(mediaURLin.includes("&") == true){ //checks for & placements
                        var temp = mediaURLin.split('&'), videoin = temp[0], timein = temp[1]; //splits the & with video id and time in
                        timein = timein.replace("t=","").replace("s","") //strips the time in to a var
                    } else if(mediaURLin.includes("?") == true){ //checks for ? placements
                        var temp = mediaURLin.split('?'), videoin = temp[0], timein = temp[1]; //splits the ? with video id and time in
                        timein = timein.replace("t=","").replace("s","") //strips the time in to a var
                    } else { videoin = mediaURLin; timein = 0 }; //defaults to it being the video ID and starts video from beginning
                    player.loadVideoById({'videoId':videoin,'startSeconds': timein})//loads video by video ID, and plays time based on input
                    inprompt = inprompt + "Playing: $video = " + videoin; doubleln() //prints video playing to console
                } else { var index = 0, temp = mediaURLin.split('&'); //runs this if there is a playlist ID,
                    for(var i = 0; i < temp.length; i++){ temp[i] = temp[i].replace("list=", "") //splits it and counts how many times there's a & to check.
                        if(mediaURLin.includes("t=") == true){timein = temp[i].replace("t=","").replace("s","")} //stores the time data to a var
                        if(temp[i].includes("index=") == true){temp[i] = temp[i].replace("index=",""); index = temp[i]; index = -(-index + 2)} //replaces playlist data and stores to index
                        if(temp[i].length == 34){ loadPlaylist = temp[i] } //recognises if it's only a playlist ID.
                        inprompt = inprompt + i + ":" + temp[i] + " "; println()} //prints to consoles the array of stored vars.
                    player.loadPlaylist({'list':loadPlaylist, 'index': index, 'startSeconds': timein}); //plays playlist with index var, and time.
                    inprompt = inprompt + "Playing: $playlist:" + index + "&" + loadPlaylist; doubleln()}} //prints to console the playlist ID
            

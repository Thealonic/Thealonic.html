            function forwardVideo(){player.seekTo((player.getCurrentTime() + currentskiptime)); 
                var skipamount = 1; ptxt.textContent = "Skipped " + (skipamount * currentskiptime) + " seconds ahead";
                counter=setInterval(function() {skipamount++; player.seekTo((player.getCurrentTime() + currentskiptime))
                ptxt.textContent = "Skipped " + (skipamount * currentskiptime) + " seconds ahead"; playstateupdateforward(); 
                playerspeed.textContext = currentskiptime}, 200)}
            function rewindVideo(){player.seekTo((player.getCurrentTime() - currentskiptime)); 
                var skipamount = 1; ptxt.textContent = "Rewinded " + (skipamount * currentskiptime) + " seconds";
                counter=setInterval(function() {skipamount++; player.seekTo((player.getCurrentTime() - currentskiptime))
                ptxt.textContent = "Rewinded " + (skipamount * currentskiptime) + " seconds"; playstateupdatebackward(); 
                playerspeed.textContext = currentskiptime}, 200)}
            function end(){clearInterval(counter)}
                

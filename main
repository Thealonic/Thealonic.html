            function updateClock(){var date = new Date(), mins = date.getMinutes(), hrs = date.getHours(), ext = "AM";
                if(hrs > 12){ var ext = "PM"; var hrs = hrs - parseInt(12)}
                if(hrs < 10){var hrs = "00"+hrs} if(mins < 10){var mins = "0"+mins}

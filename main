            function updateClock(){
                var date = new Date(), mins = date.getMinutes(), hrs = date.getHours(), ext = "AM";
                if(hrs > 12){var ext="PM",hrs=hrs-parseInt(12)}
                if(hrs < 10){var hrs="0"+hrs}; if(mins < 10){var mins="0"+mins}
                var currentTime=hrs+":"+mins+" "+ext; $('#showClock').html(currentTime)}
            
        // Drag element scripts ------------------------------------------------------------------------------>
            dragElement(document.getElementById("cat")); dragElement(document.getElementById("pyro")); dragElement(document.getElementById("disk")); dragElement(document.getElementById("message")); 
            dragElement(document.getElementById("embed")); dragElement(document.getElementById("note")); dragElement(document.getElementById("prompt")); dragElement(document.getElementById("notepad"));
            dragElement(document.getElementById("logoff")); dragElement(document.getElementById("internet"));
            
            function dragElement(elmnt) { var pos1 = 0, pos2 = 0, pos3 = 0, pos4 = 0;
                if (document.getElementById(elmnt.id + "window")) {document.getElementById(elmnt.id + "window").onmousedown = dragMouseDown} else {elmnt.onmousedown = dragMouseDown}
            function dragMouseDown(e) { e = e || window.event; e.preventDefault(); pos3 = e.clientX; pos4 = e.clientY; document.onmouseup = closeDragElement; document.onmousemove = elementDrag}
            function elementDrag(e) { e = e || window.event;  e.preventDefault(); pos1 = pos3 - e.clientX; pos2 = pos4 - e.clientY; pos3 = e.clientX; pos4 = e.clientY;
                elmnt.style.top = (elmnt.offsetTop - pos2) + "px"; elmnt.style.left = (elmnt.offsetLeft - pos1) + "px";} //if(elmnt.offsetTop < 0){elmnt.style.top = elmnt.offsetTop - pos2}
            function closeDragElement() {document.onmouseup = null; document.onmousemove = null}}
        </script>

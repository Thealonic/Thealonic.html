            function updateClock(){
                var mins = da.getMinutes() + minutemath; var hrs = da.getHours() + hourmath; var ext = "AM";
                if(hrs > 12){ var ext = "PM"; var hrs = hrs - parseInt(12); }
                if(hrs < 10){ var hrs = "0"+hrs; } if(mins < 10){ var mins = "0"+mins; }
                var currentTime = hrs + ":" + mins + " " + ext;
                $('#showClock').html(currentTime); }
                
//-- clock for clockbox ------------------------------------------------------------------------------>
                
            var canvas = document.getElementById("canvas");
            var ctx = canvas.getContext("2d"); var radius = canvas.height / 2;
            ctx.translate(radius, radius); radius = radius * 0.90; setInterval(drawClock, 1000);
            
            function drawClock() { drawFace(ctx, radius); drawTime(ctx, radius)}
            function drawFace(ctx, radius) { ctx.beginPath(); ctx.arc(0, 0, radius, 0, 2*Math.PI); ctx.fillStyle = curcolsilver; ctx.fill();}
            
            function drawTime(ctx, radius){ 
                var now = new Date(); hour = now.getHours(); minute = now.getMinutes() + minutemath; second = now.getSeconds();
                hour=hour%12; hour=(hour*Math.PI/6)+(minute*Math.PI/(6*60))+(second*Math.PI/(360*60));
                
                drawHand(ctx, hour, radius*0.5, radius*0.05); //hand drawing functions
                minute=(minute*Math.PI/30)+(second*Math.PI/(30*60)); drawHand(ctx, minute, radius*0.8, radius*0.05);
                second=(second*Math.PI/30); drawHand(ctx, second, radius*0.9, radius*0.005)} //hand drawing functions
            
            var hand = 1;
            function drawHand(ctx, pos, length, width) {
            	if (hand < 3){ ctx.beginPath(); ctx.lineWidth = 2.5; ctx.strokeStyle = curcolwincyan;
                  ctx.moveTo(0,0); ctx.rotate(pos); ctx.lineTo(0, -length); ctx.stroke(); ctx.rotate(-pos); hand = hand + 1;
            	} else { ctx.beginPath(); ctx.lineWidth = 0.5; ctx.strokeStyle = curcolblack; ctx.lineCap = "round"; 
                  ctx.moveTo(0,0); ctx.rotate(pos); ctx.lineTo(0, -length); ctx.stroke(); ctx.rotate(-pos); hand = 1 }}
            

            var canvas = document.getElementById("canvas"), ctx = canvas.getContext("2d"), radius = canvas.height / 2, hand = 1;
            ctx.translate(radius, radius); radius = radius * 0.90; setInterval(drawClock, 1000);
            function drawClock() { drawFace(ctx, radius); drawTime(ctx, radius)}
            function drawFace(ctx, radius) { ctx.beginPath(); ctx.arc(0, 0, radius, 0, 2*Math.PI); ctx.fillStyle = ccsilver; ctx.fill()}
            
            function drawTime(ctx, radius){ var now = new Date(); var hour = now.getHours(); var minute = now.getMinutes() + minutemath; var second = now.getSeconds();
                hour=hour%12; hour=(hour*Math.PI/6)+(minute*Math.PI/(6*60))+(second*Math.PI/(360*60)); drawHand(ctx, hour, radius*0.5, radius*0.05);
                minute=(minute*Math.PI/30)+(second*Math.PI/(30*60)); drawHand(ctx, minute, radius*0.8, radius*0.05);
                second=(second*Math.PI/30); drawHand(ctx, second, radius*0.9, radius*0.005)} //hand drawing functions

            function drawHand(ctx, pos, length, width) {
            	if (hand < 3){ ctx.beginPath(); ctx.lineWidth = 2.5; ctx.strokeStyle = ccwincyan; ctx.moveTo(0,0); ctx.rotate(pos); ctx.lineTo(0, -length); ctx.stroke(); ctx.rotate(-pos); hand = hand + 1
            	} else { ctx.beginPath(); ctx.lineWidth = 0.5; ctx.strokeStyle = ccblack; ctx.lineCap = "round"; ctx.moveTo(0,0); ctx.rotate(pos); ctx.lineTo(0, -length); ctx.stroke(); ctx.rotate(-pos); hand = 1}}
            

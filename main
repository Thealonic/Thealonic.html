                    function tasktoggle(){ //function that's called with a cur task number every time a task on the taskbar is clicked.
                        if(ButtonOpen[curtask]==false){ //checks if button of current task is already pressed in.
                            if(curtask==1){tumblrminimize()//if the task clicked on is tumblr, minimize the tumblr feed. (Because tasks 0-2 are not programmed by default.)
                            }else if(curtask==2){minimizewindow();curtask=1;minimizewindow() //if the pyro windows are already active, minimizes both active (Because tasks 0-2 are not programmed by default.)
                            }else{minimizewindow()}ButtonOpen[curtask]=true //else, treat as a regular window.
                        }else if(ButtonOpen[curtask]==true){ //checks if button of current task is already pressed in.
                            if(curtask==1){tumblractive() //if the task clicked on is tumblr, makes tumblr the active window. (Because tasks 0-2 are not programmed by default.)
                            }else if(curtask==2){ //if the pyro windows are already active, minimizes both active (Because tasks 0-2 are not programmed by default.)
                              if(window2==true){curtask=2;currentwindow();cat.marginLeft="0px";cat.zIndex=cal} 
                              if(window1==true){curtask=1;currentwindow();pyro.marginLeft="0px";pyro.zIndex=cal}  //checks which windows are currently active, brings the prioritised window to the front.
                            }else{currentwindow()}ButtonOpen[curtask]=false} //else, treat as a regular window.
                        curtask=0;menuclosed()} //resets the current active task and makes the start menu closed.

            function playstateupdateforward() { var playerTotalTime = player.getDuration(); var playerCurrentTime = player.getCurrentTime() + currentskiptime;
                if(playerTotalTime > playerCurrentTime){ var playerTimeDifference = (playerCurrentTime / playerTotalTime) * 100; progress(playerTimeDifference, $('#progressBar'))} else {nextVideo()}} //if #ref0038 > 100, skip video.
            function playstateupdatebackward() {var playerTotalTime = player.getDuration(); var playerCurrentTime = player.getCurrentTime() - currentskiptime;
                var playerTimeDifference = (playerCurrentTime / playerTotalTime) * 100; progress(playerTimeDifference, $('#progressBar'))} //enforces #ref0038 update
            function playstateupdate(){ var playerTotalTime = player.getDuration(); var playerCurrentTime = player.getCurrentTime();
                var playerTimeDifference = (playerCurrentTime / playerTotalTime) * 100; progress(playerTimeDifference, $('#progressBar'))} //enforces #ref0038 update
            
            function playstate() {playertext.textContent = shuffletext + "Playing"} //enforces player state as playing.
            //#ref0041: Updates the console at the bottom left with the current players state.
            function changeplayerstate(playerStatus) {
                if (playerStatus == -1) { setTimeout(function() {playertext.textContent = shuffletext + "Stopped"}, 200)
                } else if (playerStatus == 0) { setTimeout(function() {playertext.textContent = shuffletext + "Ended"}, 200)
                } else if (playerStatus == 1) { setTimeout(function() {playertext.textContent = shuffletext + "Playing"}, 500)
                } else if (playerStatus == 2) { setTimeout(function() {playertext.textContent = shuffletext + "Paused"}, 200)
                } else if (playerStatus == 3) { playerupdate(); setTimeout(function() {playertext.textContent = shuffletext + "Buffering";}, 200)
                } else if (playerStatus == 5) { setTimeout(function() {playertext.textContent = shuffletext + "Queued"}, 200)}}
            function onPlayerError(event) {player.nextVideo(); playertext.textContent = "Youtube loading error on previous video, video has been skipped."}

            function onPlayerStateChange(event) { changeplayerstate(event.data);
                if (event.data == YT.PlayerState.PLAYING) { var playerTotalTime = player.getDuration();
                    mytimer = setInterval(function() { 
                        var playerCurrentTime = player.getCurrentTime(); var playerTimeDifference = (playerCurrentTime / playerTotalTime) * 100; 
                        if (playerCurrentTime < playerTotalTime){
                        progress(playerTimeDifference, $('#progressBar'))}}, 100)
                } else {clearTimeout(mytimer)}} //#ref0038

            function playstateupdateforward() { var playerTotalTime = player.getDuration(); var playerCurrentTime = player.getCurrentTime() + currentskiptime;
                if(playerTotalTime > playerCurrentTime){ var playerTimeDifference = (playerCurrentTime / playerTotalTime) * 100; progress(playerTimeDifference, $('#progressBar'))} else {nextVideo()}}
            function playstateupdatebackward() {var playerTotalTime = player.getDuration(); var playerCurrentTime = player.getCurrentTime() - currentskiptime;
                var playerTimeDifference = (playerCurrentTime / playerTotalTime) * 100; progress(playerTimeDifference, $('#progressBar'))}
            function playstateupdate(){ var playerTotalTime = player.getDuration(); var playerCurrentTime = player.getCurrentTime();
                var playerTimeDifference = (playerCurrentTime / playerTotalTime) * 100; progress(playerTimeDifference, $('#progressBar'))}

            function changeplayerstate(playerStatus) {
                if (playerStatus == -1) { setTimeout(function() {ptxt.textContent = shuffletext + "Stopped"}, 200)
                } else if (playerStatus == 0) { setTimeout(function() {ptxt.textContent = shuffletext + "Ended"}, 200)
                } else if (playerStatus == 1) { setTimeout(function() {ptxt.textContent = shuffletext + "Playing"}, 500)
                } else if (playerStatus == 2) { setTimeout(function() {ptxt.textContent = shuffletext + "Paused"}, 200)
                } else if (playerStatus == 3) { playerupdate(); setTimeout(function() {ptxt.textContent = shuffletext + "Buffering"}, 200)
                } else if (playerStatus == 5) { setTimeout(function() {ptxt.textContent = shuffletext + "Queued"}, 200)}}
            function onPlayerError(event) {player.nextVideo(); ptxt.textContent = "Youtube loading error on previous video, video has been skipped."}

            function onPlayerStateChange(event) { changeplayerstate(event.data); if (event.data == YT.PlayerState.PLAYING) { var playerTotalTime = player.getDuration();

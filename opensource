        <script> 
            function diskactive() { 
                idleactive(); inactiveall(); diskButtonOpen = false;
                document.getElementById("disk").style.zIndex = (currentno = currentno + 1); currentno = currentno + 1; 
                document.getElementById("diskwindow").style.backgroundImage = discord + "630047188520665121/791321110750298162/title-bar_postheader.png')";
                document.getElementById("taskapp3").style.backgroundImage = discord + "630047188520665121/789416557692452884/Taskbar_application.png')";} 
                
            function diskclose() { diskButtonOpen = true;
                document.getElementById("disk").style.marginLeft = "-20000px";
                document.getElementById("disk").style.right = "-100%";
                document.getElementById("taskapp3").style.marginLeft = "-20000px";
                document.getElementById("taskapp3").style.backgroundImage = discord + "630047188520665121/789420847604564008/Taskbar_application1.png')";
                idleactive(); inactiveall();}
            
            function diskopen() { 
                idleactive(); inactiveall(); diskButtonOpen = false;
                document.getElementById("disk").style.zIndex = (currentno = currentno + 1); currentno = currentno + 1; 
                document.getElementById("disk").style.marginLeft = "0px";
                document.getElementById("disk").style.right = "3%";
                document.getElementById("diskwindow").style.backgroundImage = discord + "630047188520665121/791321110750298162/title-bar_postheader.png')";
                document.getElementById("taskapp3").style.backgroundImage = discord + "630047188520665121/789416557692452884/Taskbar_application.png')";
                document.getElementById("taskapp3").style.marginLeft = "4px";}
                
        </script> 
        

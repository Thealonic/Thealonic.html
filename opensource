<!-- On page load handling ------------------------------------------------------------------------------>
        
        <script>
            $(document).ready(function(){
                var infScr = $('body').hasClass('infinite_scrolling'); twoCol = $('body').hasClass('two_column'); $container = $('#desktop');
                if( infScr && twoCol ){ $container.imagesLoaded(function(){ $container.masonry({ itemSelector: '.window'}) });
                    $container.infinitescroll({ itemSelector: '.window'}, function( newElements ) {
                        var $newElems = $( newElements ).css({ opacity: 0 }); $newElemsIDs = $newElems.map(function () { return this.id; }).get();
                        Tumblr.LikeButton.get_status_by_post_ids($newElemsIDs);
                        $newElems.imagesLoaded(function(){ $container.masonry( 'appended', $newElems, true ); $newElems.animate({ opacity: 1 })})
                        $newElems.find('.video').each( function() { resizeVideos() })})}
                //taskbar clock functions
                window.setInterval(updateClock(), 6000);
                updateClock()});
            
            function updateClock(){
                var mins = da.getMinutes() + minutemath; var hrs = da.getHours() + hourmath; var ext = "AM";
                if(hrs > 12){ var ext = "PM"; var hrs = hrs - parseInt(12); }
                if(hrs < 10){ var hrs = "0"+hrs; } if(mins < 10){ var mins = "0"+mins; }
                var currentTime = hrs + ":" + mins + " " + ext;
                $('#showClock').html(currentTime); }
                
//-- clock for clockbox ------------------------------------------------------------------------------>
                
            var canvas = document.getElementById("canvas");
            var ctx = canvas.getContext("2d"); var radius = canvas.height / 2;
            ctx.translate(radius, radius); radius = radius * 0.90; setInterval(drawClock, 1000);
            
            function drawClock() { drawFace(ctx, radius); drawTime(ctx, radius)}
            function drawFace(ctx, radius) { ctx.beginPath(); ctx.arc(0, 0, radius, 0, 2*Math.PI); ctx.fillStyle = "#c0c0c0";ctx.fill();}
            
            function drawTime(ctx, radius){ 
                var now = new Date(); hour = now.getHours(); minute = now.getMinutes() + minutemath; second = now.getSeconds();
                hour=hour%12; hour=(hour*Math.PI/6)+(minute*Math.PI/(6*60))+(second*Math.PI/(360*60));
                
                drawHand(ctx, hour, radius*0.5, radius*0.05); //hand drawing functions
                minute=(minute*Math.PI/30)+(second*Math.PI/(30*60)); drawHand(ctx, minute, radius*0.8, radius*0.05);
                second=(second*Math.PI/30); drawHand(ctx, second, radius*0.9, radius*0.005)} //hand drawing functions
            
            var hand = 1;
            function drawHand(ctx, pos, length, width) {
            	if (hand < 3){ ctx.beginPath(); ctx.lineWidth = 2.5; ctx.strokeStyle = "#008080";
                  ctx.moveTo(0,0); ctx.rotate(pos); ctx.lineTo(0, -length); ctx.stroke(); ctx.rotate(-pos); hand = hand + 1;
            	} else { ctx.beginPath(); ctx.lineWidth = 0.5; ctx.strokeStyle = "#000"; ctx.lineCap = "round"; 
                  ctx.moveTo(0,0); ctx.rotate(pos); ctx.lineTo(0, -length); ctx.stroke(); ctx.rotate(-pos); hand = 1 }}
            
        // Drag element scripts ------------------------------------------------------------------------------>
            
            dragElement(document.getElementById("cat")); dragElement(document.getElementById("pyro")); dragElement(document.getElementById("disk")); dragElement(document.getElementById("message"));
            dragElement(document.getElementById("logoff")); dragElement(document.getElementById("shutdown")); dragElement(document.getElementById("clock"));
                
            function dragElement(elmnt) { var pos1 = 0, pos2 = 0, pos3 = 0, pos4 = 0;
            if (document.getElementById(elmnt.id + "window")) { document.getElementById(elmnt.id + "window").onmousedown = dragMouseDown; } else { elmnt.onmousedown = dragMouseDown; }
            function dragMouseDown(e) { //mouse moment grabbing
                e = e || window.event; e.preventDefault(); pos3 = e.clientX; pos4 = e.clientY;
                document.onmouseup = closeDragElement; document.onmousemove = elementDrag; }
            function elementDrag(e) { e = e || window.event;  e.preventDefault();
               pos1 = pos3 - e.clientX; pos2 = pos4 - e.clientY; pos3 = e.clientX; pos4 = e.clientY;
                elmnt.style.top = (elmnt.offsetTop - pos2) + "px"; elmnt.style.left = (elmnt.offsetLeft - pos1) + "px"; }
            function closeDragElement() { document.onmouseup = null; document.onmousemove = null; }}
        
//-- Startmenu script handling ------------------------------------------------------------------------------>
            
            var progmenu = document.getElementById("menuprograms"); progsubmenu = document.getElementById("programsmenu"); accemenu = document.getElementById("accessories"); accesubmenu = document.getElementById("menuaccessoriessubmenu");
                commmenu = document.getElementById("communications"); commsubmenu = document.getElementById("menucommunicationssubmenu"); entemenu = document.getElementById("entertainment"); entesubmenu = document.getElementById("menuentertainmentsubmenu");
                gamemenu = document.getElementById("games"); gamesubmenu = document.getElementById("gamessubmenu"); intemenu = document.getElementById("internettools"); intesubmenu = document.getElementById("internettoolssubmenu");
                systmenu = document.getElementById("systemtools"); systsubmenu = document.getElementById("systemtoolssubmenu"); onlimenu = document.getElementById("onlineservices"); onlisubmenu = document.getElementById("onlineservicessubmenu");
                starmenu = document.getElementById("startup"); starsubmenu = document.getElementById("startupsubmenu"); favomenu = document.getElementById("menufavourites"); favosubmenu = document.getElementById("favouritesmenu");
                chanmenu = document.getElementById("channels"); chansubmenu = document.getElementById("channelssubmenu"); linkmenu = document.getElementById("links"); linksubmenu = document.getElementById("linkssubmenu");
                medimenu = document.getElementById("media"); medisubmenu = document.getElementById("mediasubmenu"); documenu = document.getElementById("menudocuments"); docusubmenu = document.getElementById("documentsmenu");
                settmenu = document.getElementById("menusettings"); settsubmenu = document.getElementById("settingsmenu"); actimenu = document.getElementById("activedesktop"); actisubmenu = document.getElementById("activedesktopsubmenu");
                findmenu = document.getElementById("menufind"); findsubmenu = document.getElementById("findmenu");
            var white = "#fff"; black = "#000"; prus = "#00207c"; silv = "#c0c0c0"; faraw = "200000px"; aropen = "-134px -216px"; arclose = "-54px -216px"; yell = "#ffff00";
            
            function cmw() {cm.style.color = white} function cmb() {cm.style.color = black} function cmd() {cms.style.marginLeft = faraw} function cmbp() {cm.style.backgroundColor = silv} function cmbs() {cm.style.backgroundColor = prus}
            function progcm() {cm = progmenu; cms = progsubmenu} function accecm() {cm = accemenu; cms = accesubmenu} function commcm() {cm = commmenu; cms = commsubmenu} function entecm() {cm = entemenu; cms = entesubmenu} 
            function gamecm() {cm = gamemenu; cms = gamesubmenu} function intecm() {cm = intemenu; cms = intesubmenu} function systcm() {cm = systmenu; cms = systsubmenu} function onlicm() {cm = onlimenu; cms = onlisubmenu} 
            function starcm() {cm = starmenu; cms = starsubmenu} function favocm() {cm = favomenu; cms = favosubmenu} function chancm() {cm = chanmenu; cms = chansubmenu} function linkcm() {cm = linkmenu; cms = linksubmenu} 
            function medicm() {cm = medimenu; cms = medisubmenu} function docucm() {cm = documenu; cms = docusubmenu} function settcm() {cm = settmenu; cms = settsubmenu} function acticm() {cm = actimenu; cms = actisubmenu} 
            function findcm() {cm = findmenu; cms = findsubmenu} function starton() {cmbs(); cmw()} function startoff(){cmbp(); cmb()}

            

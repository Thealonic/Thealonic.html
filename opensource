            function programsopen() { 
                document.getElementById("programsmenu").style.marginLeft = "163px"; 
                document.getElementById("menuprograms").style.backgroundColor = "#00207c";
                document.getElementById("menuprograms").style.color = "#ffffff"; 
                document.getElementById("menuarrow1").style.backgroundPosition = "-134px -215px";}
            function programsclosed() { 
                document.getElementById("programsmenu").style.marginLeft = "200000px"; 
                document.getElementById("menuprograms").style.backgroundColor = "#c0c0c0";
                document.getElementById("menuprograms").style.color = "#000000"; 
                document.getElementById("menuarrow1").style.backgroundPosition = "-150px -215px";}

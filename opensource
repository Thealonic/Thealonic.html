                        function precolourupdate() {
                            precolwhite = curcolwhite; precollightg = curcollightg; precolsilver = curcolsilver; precolmidgray = curcolmidgray; precolgray = curcolgray; precolblack = curcolblack; precolblue = curcolblue;
                            precolprussian = curcolprussian; precolwincyan = curcolwincyan; precolyellow = curcolyellow; precolfont = curcolfont; precolfonttitle = curcolfonttitle; precolhover = curcolhover;}
                        function themechange() { document.documentElement.style.setProperty('--blue', curcolblue); 
                            currentinactiveheader = precurrentinactiveheader; currentactiveheader = precurrentactiveheader; document.getElementById("diskwindow").style.backgroundImage = currentactiveheader;
                            document.documentElement.style.setProperty('--white', curcolwhite); document.documentElement.style.setProperty('--gray', curcolgray); document.documentElement.style.setProperty('--lightg', curcollightg); 
                            document.documentElement.style.setProperty('--black', curcolblack); document.documentElement.style.setProperty('--silver', curcolsilver); document.documentElement.style.setProperty('--prussian', curcolprussian);
                            document.documentElement.style.setProperty('--midgray', curcolmidgray); document.documentElement.style.setProperty('--wincyan', curcolwincyan); document.documentElement.style.setProperty('--yellow', curcolyellow); 
                            document.documentElement.style.setProperty('--font', curcolfont); document.documentElement.style.setProperty('--fonttitle', curcolfonttitle); document.documentElement.style.setProperty('--hover', curcolhover);}
                        function themepreviewchange() { document.documentElement.style.setProperty('--preblue', precolblue);
                            document.documentElement.style.setProperty('--prewhite', precolwhite); document.documentElement.style.setProperty('--pregray', precolgray); document.documentElement.style.setProperty('--prelightg', precollightg); 
                            document.documentElement.style.setProperty('--preblack', precolblack); document.documentElement.style.setProperty('--presilver', precolsilver); document.documentElement.style.setProperty('--preprussian', precolprussian);
                            document.documentElement.style.setProperty('--premidgray', precolmidgray); document.documentElement.style.setProperty('--prewincyan', precolwincyan); document.documentElement.style.setProperty('--preyellow', precolyellow); 
                            document.documentElement.style.setProperty('--prefont', precolfont); document.documentElement.style.setProperty('--prefonttitle', precolfonttitle); document.documentElement.style.setProperty('--prehover', precolhover);}
                    
                        function defaultcolourscheme() { 
                            curcolwhite = "#ffffff";   curcollightg = "#dfdfdf";   curcolsilver = "#c0c0c0"; curcolmidgray = "#ababab"; curcolgray = "#808080"; curcolblack = "#0a0a0a"; curcolblue = "#0000ff";

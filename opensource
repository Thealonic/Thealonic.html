                        <script>
                            var colourpicked = false; currentcolour = "#008080"; fontcolour = "#ffffff"; dcb1 = document.getElementById("dcb1"); dcb2 = document.getElementById("dcb2");
                            var cpo = document.getElementById("currentpickedo"); cpc = document.getElementById("currentpickedc"); ap = document.getElementById("apperancepreview");
                                desktop1 = document.getElementById("desktopi1"); desktop2 = document.getElementById("desktopi2"); desktop3 = document.getElementById("desktopi3"); 
                                desktop4 = document.getElementById("desktopi4"); desktop5 = document.getElementById("desktopi5"); desktop6 = document.getElementById("desktopi6"); 
                                desktop8 = document.getElementById("desktopi8"); desktop9 = document.getElementById("desktopi9"); 
                            
                            function colourpickerupdate(){ cpc.style.backgroundColor = currentcolour; cpo.style.backgroundColor = currentcolour; }
                            function colourpicker(){ if (colourpicked == false){ document.getElementById("coloursampler").style.marginLeft = "    -2px"; colourpicked =  true;
                                              } else if (colourpicked == true ){ document.getElementById("coloursampler").style.marginLeft = "-20000px"; colourpicked = false; }}
                            function colourpickerexit() { document.getElementById("coloursampler").style.marginLeft = "-20000px"; colourpicked = false; }

                            function backcolourset(){ //changes desktop icon span colour + backround colour.
                                desktop1.style.backgroundColor = currentcolour; desktop2.style.backgroundColor = currentcolour; desktop3.style.backgroundColor = currentcolour;
                                desktop4.style.backgroundColor = currentcolour; desktop5.style.backgroundColor = currentcolour; desktop6.style.backgroundColor = currentcolour;
                                desktop8.style.backgroundColor = currentcolour; document.body.style.backgroundColor = currentcolour;
                                if(ctoggle == true){ document.body.style.backgroundColor = "#000"; document.getElementById("iconpreview").style.backgroundColor = "#000";
                                                     cpc.style.backgroundColor = "#000"; cpo.style.backgroundColor = "#000"; ap.style.backgroundColor = "#000";}} 
                            function fontcolourset(){ // changes desktop icon text color;
                                desktop1.style.color = fontcolour; desktop2.style.color = fontcolour; desktop3.style.color = fontcolour;
                                desktop4.style.color = fontcolour; desktop5.style.color = fontcolour; desktop6.style.color = fontcolour;
                                desktop8.style.color = fontcolour;}
                                
                            function backcolourpreview(){ // changes preview desktop icons background colour + background colours in appearance tab.
                                document.getElementById("iconpre1").style.backgroundColor = currentcolour; document.getElementById("iconpre2").style.backgroundColor = currentcolour; 
                                document.getElementById("iconpre3").style.backgroundColor = currentcolour; document.getElementById("iconpre4").style.backgroundColor = currentcolour; 
                                document.getElementById("iconpre6").style.backgroundColor = currentcolour; document.getElementById("iconpreview").style.backgroundColor = currentcolour;  
                                ap.style.backgroundColor = currentcolour; fontcolorsetpreview(); dcb1.style.backgroundColor = currentcolour;  dcb2.style.backgroundColor = currentcolour; }
                            function fontcolorsetpreview(){ // changes preview desktop icons span colour
                                document.getElementById("iconpre1").style.color = fontcolour; document.getElementById("iconpre2").style.color = fontcolour; 
                                document.getElementById("iconpre3").style.color = fontcolour; document.getElementById("iconpre4").style.color = fontcolour; 
                                document.getElementById("iconpre6").style.color = fontcolour;}
                            
                            function cpick1() {   currentcolour = "#ffffff"; fontcolour = "#000"; colourpickerupdate(); document.getElementById("iconpre2").style.backgroundColor = currentcolour;}
                            function cpick2() {   currentcolour = "#000000"; fontcolour = "#fff"; colourpickerupdate(); ctoggle = false; backcolourpreview();}
                            function cpick3() {   currentcolour = "#c2c6ca"; fontcolour = "#000"; colourpickerupdate(); ctoggle = false; backcolourpreview();}
                            function cpick4() {   currentcolour = "#85898d"; fontcolour = "#fff"; colourpickerupdate(); ctoggle = false; backcolourpreview();}
                            function cpick5() {   currentcolour = "#ff0000"; fontcolour = "#fff"; colourpickerupdate(); ctoggle = false; backcolourpreview();}
                            function cpick6() {   currentcolour = "#aa0055"; fontcolour = "#fff"; colourpickerupdate(); ctoggle = false; backcolourpreview();}
                            function cpick7() {   currentcolour = "#ffff00"; fontcolour = "#000"; colourpickerupdate(); ctoggle = false; backcolourpreview();}
                            function cpick8() {   currentcolour = "#aaaa55"; fontcolour = "#fff"; colourpickerupdate(); ctoggle = false; backcolourpreview();}
                            function cpick9() {   currentcolour = "#00ff00"; fontcolour = "#000"; colourpickerupdate(); ctoggle = false; backcolourpreview();}
                            function cpick10() {  currentcolour = "#00aa55"; fontcolour = "#fff"; colourpickerupdate(); ctoggle = false; backcolourpreview();}
                            function cpick11() {  currentcolour = "#00ffff"; fontcolour = "#000"; colourpickerupdate(); ctoggle = false; backcolourpreview();}
                            function cpick12() {  currentcolour = "#008080"; fontcolour = "#fff"; colourpickerupdate(); ctoggle = false; backcolourpreview();}
                            function cpick13() {  currentcolour = "#0000ff"; fontcolour = "#fff"; colourpickerupdate(); ctoggle = false; backcolourpreview();}
                            function cpick14() {  currentcolour = "#0000aa"; fontcolour = "#fff"; colourpickerupdate(); ctoggle = false; backcolourpreview();}
                            function cpick15() {  currentcolour = "#ff00ff"; fontcolour = "#000"; colourpickerupdate(); ctoggle = false; backcolourpreview();}
                            function cpick16() {  currentcolour = "#aa55aa"; fontcolour = "#fff"; colourpickerupdate(); ctoggle = false; backcolourpreview();}
                            

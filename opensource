                    <div class="taskbaricon3" style="cursor:pointer;" onclick="messageopen()();"></div> <!-- active desktop icon -->
                    <div class="taskbaricon4"></div></div>
                <taskbar class="big_seperator"> </taskbar>
                <taskbar class="small_seperator" style="cursor:e-resize"> </taskbar>
                
                <div class="taskbarapp1" style="cursor:pointer;" onmousedown="tumblrtoggle(); startpressdelay()" id="taskapp1"> <!--position: absolute; margin-left:172px-->
                        <div class="title-bar-text" style="color:black; margin-top:2px; font-size: 11px;">🍭 very.vunnies</div></div>
                <div class="taskbarapp2" style="cursor:pointer;" onmousedown="pyrotoggle(); startpressdelay()" id="taskapp2"> <!--position: absolute; margin-left:336px-->
                        <div class="title-bar-text" style="color:black; margin-top:2px; font-size: 11px;">👿 very.unvunny</div></div>
                <div class="taskbarapp3" style="cursor:pointer;" onmousedown="disktoggle(); startpressdelay()" id="taskapp3"> <!--position: absolute; margin-left:500px-->
                        <div class="title-bar-text" style="color:black; margin-top:2px; font-size: 11px;">📺 Display Properties</div></div>
                
                <!-- ----------------------------------------------------------------------------------------------------------------------------- -->
                <!-- toggle setup                                                                                                                  -->
                <!-- ----------------------------------------------------------------------------------------------------------------------------- -->
                
                <script>
                    let tumblrButtonOpen = false
                    let pyroButtonOpen = true; 
                    let diskButtonOpen = true;
                
                    // tumblr post toggle
                    function tumblrtoggle() { 
                        pyroButtonOpen = true; diskButtonOpen = true;
                        
                        if (tumblrButtonOpen == false) { 
                            inactiveall(); idleactive(); tumblrButtonOpen = true;
                            document.getElementById("postwindow").style.backgroundImage = discord + "630047188520665121/791328235451318342/title-bar_postheaderinactive.png')";
                            document.getElementById("taskapp1").style.backgroundImage = discord + "630047188520665121/789420847604564008/Taskbar_application1.png')";
                            document.getElementById("desktop").style.left = "-20000px"; document.body.style.overflowY = "hidden";
                            
                        } else if (tumblrButtonOpen == true){ 
                            inactiveall(); tumblrButtonOpen = false;
                            document.getElementById("post").style.zIndex = (currentno = currentno + 1); currentno = currentno + 1;
                            document.getElementById("postwindow").style.backgroundImage = discord + "630047188520665121/791321110750298162/title-bar_postheader.png')";
                            document.getElementById("taskapp1").style.backgroundImage = discord + "630047188520665121/789416557692452884/Taskbar_application.png')";
                            document.getElementById("desktop").style.left = "-25px"; document.body.style.overflowY = "scroll";}}
                    
                    // random message box active toggle
                    function pyrotoggle() { 
                        tumblrButtonOpen = true; diskButtonOpen = true;
                        if (pyroButtonOpen == false) { 
                            inactiveall(); idleactive(); pyroButtonOpen = true;
                            document.getElementById("pyrowindow").style.backgroundImage = discord + "630047188520665121/791328235451318342/title-bar_postheaderinactive.png')";
                            document.getElementById("catwindow").style.backgroundImage = discord + "630047188520665121/791328235451318342/title-bar_postheaderinactive.png')";
                            document.getElementById("taskapp2").style.backgroundImage = discord + "630047188520665121/789420847604564008/Taskbar_application1.png')";
                        } else if (pyroButtonOpen == true){
                            inactiveall(); pyroButtonOpen = false;
                            if (window1 == true){ 
                                document.getElementById("pyro").style.zIndex = (currentno = currentno + 1); currentno = currentno + 1;
                                document.getElementById("pyrowindow").style.backgroundImage = discord + "630047188520665121/791321110750298162/title-bar_postheader.png')";  
                                document.getElementById("taskapp2").style.backgroundImage = discord + "630047188520665121/789416557692452884/Taskbar_application.png')"; 
                            } else if (window2 == true){ 
                                document.getElementById("cat").style.zIndex = (currentno = currentno + 1); currentno = currentno + 1;
                                document.getElementById("catwindow").style.backgroundImage = discord + "630047188520665121/791321110750298162/title-bar_postheader.png')";  
                                document.getElementById("taskapp2").style.backgroundImage = discord + "630047188520665121/789416557692452884/Taskbar_application.png')";}}}
                    
                    // disk window active toggle
                    function disktoggle() { 
                        tumblrButtonOpen = true; pyroButtonOpen = true;
                        if (diskButtonOpen == false) { 
                            inactiveall(); idleactive(); diskButtonOpen = true;
                            document.getElementById("diskwindow").style.backgroundImage = discord + "630047188520665121/791328235451318342/title-bar_postheaderinactive.png')";
                            document.getElementById("taskapp3").style.backgroundImage = discord + "630047188520665121/789420847604564008/Taskbar_application1.png')";
                            document.getElementById("disk").style.marginTop = "-20000px";
                            
                        } else if (diskButtonOpen == true){
                            inactiveall(); diskButtonOpen = false;
                            document.getElementById("disk").style.zIndex = (currentno = currentno + 1); currentno = currentno + 1;
                            document.getElementById("disk").style.marginTop = "0px";
                            document.getElementById("diskwindow").style.backgroundImage = discord + "630047188520665121/791321110750298162/title-bar_postheader.png')";  
                            document.getElementById("taskapp3").style.backgroundImage = discord + "630047188520665121/789416557692452884/Taskbar_application.png')";}} 
                            
                    function idleactive() { tumblrButtonOpen = true;
                                              pyroButtonOpen = true; 
                                              diskButtonOpen = true; }
                </script>
                 

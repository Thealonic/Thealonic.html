        <script> 
            function diskactive() { idleactive(); inactiveall(); diskButtonOpen = false;
                document.getElementById("disk").style.zIndex = (currentlayer = currentlayer + 1); currentlayer = currentlayer + 1; 
                document.getElementById("diskwindow").style.backgroundImage = currentactiveheader;
                document.getElementById("taskapp3").style.backgroundImage = github + "img/Taskbar%20application.png')";} 
            function diskopen() { idleactive(); inactiveall(); tabreset(); diskButtonOpen = false;
                document.getElementById("disk").style.zIndex = (currentlayer = currentlayer + 1); currentlayer = currentlayer + 1; 
                document.getElementById("disk").style.marginLeft = "0px";
                document.getElementById("disk").style.top = "13%";
                document.getElementById("disk").style.left = "77%";
                document.getElementById("diskwindow").style.backgroundImage = currentactiveheader;
                document.getElementById("taskapp3").style.backgroundImage = github + "img/Taskbar%20application.png')";
                document.getElementById("taskapp3").style.marginLeft = "4px";}
            function diskclose() { diskButtonOpen = true; idleactive(); inactiveall();
                document.getElementById("disk").style.marginLeft = "-20000px";
                document.getElementById("disk").style.right = "-100%";
                document.getElementById("taskapp3").style.marginLeft = "-20000px";
                document.getElementById("taskapp3").style.backgroundImage = github + "img/Taskbar%20application1.png')";}
        </script> 
        

        <script>
            function shutdownactive(){ idleactive(); inactiveall();
                document.getElementById("shutdown").style.zIndex = (currentlayer = currentlayer + 1); currentlayer = currentlayer + 1;
                document.getElementById("shutdownwindow").style.backgroundImage = currentactiveheader; }
            function shutdown() {   document.getElementById("shutdown").style.marginLeft = "0px"; document.getElementById("shutdown").style.zIndex = "100"; }
            function shutdownno() { document.getElementById("shutdown").style.marginLeft = "200000px"; document.getElementById("shutdown").style.zIndex = "1"; }
        </script>


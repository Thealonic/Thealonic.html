                    <script>   
                        //start button / menu addressing
                        let startButtonOpen = false; var y = document.getElementById("startmenu");
                        //start button default // no start menu window
                        function startpress2() { if (y.style.marginLeft !== "200000px") { y.style.marginLeft = "200000px"; 
                                                 document.getElementById("start").style.zIndex = "4"; }}
                                
                        //start button pressed // start menu window
                        function startpress() { startButtonOpen = !startButtonOpen;
                            if (startButtonOpen) { 
                                y.style.marginLeft = "0px"; document.getElementById("start").style.zIndex = "10000000"; }}
                        function menuclosed() { 
                                y.style.marginLeft = "200000px"; document.getElementById("start").style.zIndex = "10000000"; 
                                if( startButtonOpen = false) { startButtonOpen = !startButtonOpen; }}
                        function startpressdelay(){ setTimeout(function(){
                                y.style.marginLeft = "200000px"; document.getElementById("start").style.zIndex = "10000000"; }, 200)};
                    </script> 
                        

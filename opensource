                        <script>
                            var colourpicked = false; currentcolour = "#008080"; fontcolour = "#ffffff";
                            var cpo = document.getElementById("currentpickedo"); cpc = document.getElementById("currentpickedc"); ap = document.getElementById("apperancepreview");
                            
                            var desktop1 = document.getElementById("desktopi1"); desktop2 = document.getElementById("desktopi2"); desktop3 = document.getElementById("desktopi3"); 
                                desktop4 = document.getElementById("desktopi4"); desktop5 = document.getElementById("desktopi5"); desktop6 = document.getElementById("desktopi6"); 
                                desktop7 = document.getElementById("desktopi7"); desktop8 = document.getElementById("desktopi8"); desktop9 = document.getElementById("desktopi9"); 

                            function colourpicker(){
                                       if (colourpicked == false){ document.getElementById("coloursampler").style.marginLeft = "    -2px"; colourpicked =  true;
                                } else if (colourpicked == true ){ document.getElementById("coloursampler").style.marginLeft = "-20000px"; colourpicked = false; }}
                            function colourpickerexit() { document.getElementById("coloursampler").style.marginLeft = "-20000px"; colourpicked = false; }

                            function backcolourset(){
                                document.body.style.backgroundColor = currentcolour; cpc.style.backgroundColor = currentcolour; cpo.style.backgroundColor = currentcolour; 
                                document.getElementById("iconpreview").style.backgroundColor = currentcolour;  ap.style.backgroundColor = currentcolour;
                                desktop1.style.backgroundColor = currentcolour; desktop2.style.backgroundColor = currentcolour; desktop3.style.backgroundColor = currentcolour;
                                desktop4.style.backgroundColor = currentcolour; desktop5.style.backgroundColor = currentcolour; desktop6.style.backgroundColor = currentcolour;
                                desktop7.style.backgroundColor = currentcolour; desktop8.style.backgroundColor = currentcolour;
                                document.getElementById("iconpre1").style.backgroundColor = currentcolour; document.getElementById("iconpre2").style.backgroundColor = currentcolour; 
                                document.getElementById("iconpre3").style.backgroundColor = currentcolour; document.getElementById("iconpre4").style.backgroundColor = currentcolour; 
                                document.getElementById("iconpre6").style.backgroundColor = currentcolour;} 
                                
                            function fontcolourset(){
                                desktop1.style.color = fontcolour; desktop2.style.color = fontcolour; desktop3.style.color = fontcolour;
                                desktop4.style.color = fontcolour; desktop5.style.color = fontcolour; desktop6.style.color = fontcolour;
                                desktop7.style.color = fontcolour; desktop8.style.color = fontcolour; 
                                document.getElementById("iconpre1").style.color = fontcolour; document.getElementById("iconpre2").style.color = fontcolour; 
                                document.getElementById("iconpre3").style.color = fontcolour; document.getElementById("iconpre4").style.color = fontcolour; 
                                document.getElementById("iconpre6").style.color = fontcolour;}

                            function cpick1() {   currentcolour = "#ffffff"; backcolourset(); fontcolour = "#000"; fontcolourset();}
                            function cpick2() {   currentcolour = "#000000"; backcolourset(); fontcolour = "#fff"; fontcolourset();}
                            function cpick3() {   currentcolour = "#c2c6ca"; backcolourset(); fontcolour = "#000"; fontcolourset();}
                            function cpick4() {   currentcolour = "#85898d"; backcolourset(); fontcolour = "#fff"; fontcolourset();}
                            function cpick5() {   currentcolour = "#ff0000"; backcolourset(); fontcolour = "#fff"; fontcolourset();}
                            function cpick6() {   currentcolour = "#aa0055"; backcolourset(); fontcolour = "#fff"; fontcolourset();}
                            function cpick7() {   currentcolour = "#ffff00"; backcolourset(); fontcolour = "#000"; fontcolourset();}
                            function cpick8() {   currentcolour = "#aaaa55"; backcolourset(); fontcolour = "#fff"; fontcolourset();}
                            function cpick9() {   currentcolour = "#00ff00"; backcolourset(); fontcolour = "#000"; fontcolourset();}
                            function cpick10() {  currentcolour = "#00aa55"; backcolourset(); fontcolour = "#fff"; fontcolourset();}
                            function cpick11() {  currentcolour = "#00ffff"; backcolourset(); fontcolour = "#000"; fontcolourset();}
                            function cpick12() {  currentcolour = "#008080"; backcolourset(); fontcolour = "#fff"; fontcolourset();}
                            function cpick13() {  currentcolour = "#0000ff"; backcolourset(); fontcolour = "#fff"; fontcolourset();}
                            function cpick14() {  currentcolour = "#0000aa"; backcolourset(); fontcolour = "#fff"; fontcolourset();}
                            function cpick15() {  currentcolour = "#ff00ff"; backcolourset(); fontcolour = "#000"; fontcolourset();}
                            function cpick16() {  currentcolour = "#aa55aa"; backcolourset(); fontcolour = "#fff"; fontcolourset();}
                            
                            function cpick17() {  currentcolour = "#a03762"; backcolourset(); fontcolour = "#fff"; fontcolourset();}
                            function cpick18() {  currentcolour = "#001224"; backcolourset(); fontcolour = "#fff"; fontcolourset();}
                            function cpick19() {  currentcolour = "#590f1e"; backcolourset(); fontcolour = "#fff"; fontcolourset();}
                            function cpick20() {  currentcolour = "#6d7f36"; backcolourset(); fontcolour = "#fff"; fontcolourset();}
                            
                            function cpick21() {  currentcolour = "#00ffff"; backcolourset(); fontcolour = "#000"; fontcolourset();
                                                  document.body.style.backgroundColor = "#000"; cpc.style.backgroundColor = "#000"; cpo.style.backgroundColor = "#000"; 
                                                  document.getElementById("iconpreview").style.backgroundColor = "#000";  ap.style.backgroundColor = "#000";}
                            function cpick22() {  currentcolour = "#00ff00"; backcolourset(); fontcolour = "#000"; fontcolourset();
                                                  document.body.style.backgroundColor = "#000"; cpc.style.backgroundColor = "#000"; cpo.style.backgroundColor = "#000"; 
                                                  document.getElementById("iconpreview").style.backgroundColor = "#000";  ap.style.backgroundColor = "#000";}
                            
                            function highcontrast1() { currentcolour = "#00ffff"; backcolourset(); fontcolour = "#000"; fontcolourset(); 
                                                       document.body.style.backgroundColor = "#000"; cpc.style.backgroundColor = "#000"; cpo.style.backgroundColor = "#000"; 
                                                       document.getElementById("iconpreview").style.backgroundColor = "#000";  ap.style.backgroundColor = "#000";}
                        </script>
                        

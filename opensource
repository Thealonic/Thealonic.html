        <script>
            var da = new Date(), monthArr = ["January", "February","March", "April", "May", "June", "July", "August", "September", "October", "November","December"]; realmonth = true; 
            var yearmath = 0, daymath = 0, minutemath = 0, hourmath = 0;
            
            if (minutemath > 60){ minutemath - 60; hourmath + 1;} if(minutemath < 0){ minutemath + 60; hourmath - 1;}
            if (hourmath > 24){ hourmath - 24; daymath + 1;} if(hourmath < 0){ hourmath + 24; daymath - 1;}
            
            function clocku(){ date = da.getDate() + daymath; year = da.getFullYear() + yearmath; 
                               if(realmonth == true){monthno = da.getMonth(); month = monthArr[monthno]};
                 document.getElementById("month").innerHTML = month; document.getElementById("year").innerHTML = year; }
                 
            function minuteadd() { minutemath = minutemath + 1; clocku()}   function minutesub() { minutemath = minutemath - 1; clocku()}
            function houradd() { hourmath = hourmath + 1; clocku()}         function hoursub() { hourmath = hourmath - 1; clocku()}
            function yearadd() {yearmath = yearmath + 1; clocku()}          function yearsub() {yearmath = yearmath - 1; clocku()}

            var docclock = document.getElementById("clock"); docclockwin = document.getElementById("clockwindow"); 
            function clockactive() { idleactive(); inactiveall(); docclock.style.zIndex = (currentlayer = currentlayer + 1); currentlayer = currentlayer + 1;  docclockwin.style.backgroundImage = currentactiveheader;}
            function clockclose() { idleactive(); inactiveall(); docclock.style.marginLeft = "-20000px"; docclock.style.right = "-100%"; monthdropdefa(); yeardropbackfa();}
            function clockopen() { clockactive(); docclock.style.left = "75%"; docclock.style.top = "50%"; docclock.style.marginLeft = "0px";}
        </script>
        

                document.getElementById("taskapp3").style.backgroundImage = github + "img/Taskbar%20application1.png')";
                document.getElementById("postwindow").style.backgroundImage = currentinactiveheader
                document.getElementById("taskapp1").style.backgroundImage = github + "img/Taskbar%20application1.png')";
                document.getElementById("clockwindow").style.backgroundImage = currentinactiveheader
                
                document.getElementById("messagewindow").style.backgroundImage = currentinactiveheader
                document.getElementById("logoffwindow").style.backgroundImage = currentinactiveheader
                document.getElementById("shutdownwindow").style.backgroundImage = currentinactiveheader}

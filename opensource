            function pyroshuffle(){ 
                
                if (tick !== 0){ 
                    randomNum2 = Math.floor(Math.random() * myPix.length);
                    if(randomNum2 !== randomNum){
                        randomNum = randomNum2
                        document.getElementById("pyrotext").textContent= r_text[randomNum]; 
                        document.getElementById("pyroimage").style.backgroundImage = myPix[randomNum]; randomNum = randomNum2;
                    } else { randomNum2 = Math.floor(Math.random() * myPix.length);
                        randomNum = randomNum2;
                        document.getElementById("pyrotext").textContent= r_text[randomNum]; 
                        document.getElementById("pyroimage").style.backgroundImage = myPix[randomNum]; randomNum = randomNum2;}
                        
                    if(tick == 3){ document.getElementById("pyrobutton").textContent = "Probably!";}
                    if(tick == 2){ document.getElementById("pyrobutton").textContent = "Maybe?";}
                    if(tick == 1){ document.getElementById("pyrobutton").textContent = "No.";}
                    
                    if(randomNum == 30){ document.getElementById("pyrobutton").textContent="Nuzzles";}
                    tick = tick - 1;
                    
                } else { 
                    document.getElementById("pyro").style.marginLeft = "-20000px"; pyrotabs = pyrotabs - 1;
                    document.getElementById("pyrobutton").textContent = "Definately"; document.getElementById("pyrotext").textContent = '💯 Wubba lubba dub dub!';
                    document.getElementById("pyroimage").style.backgroundImage = "var(--defaultpyro)"; pyroclose(); }}
                

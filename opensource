<!-- toggle setup ------------------------------------------------------------------------------>
                
                <script>
                    let tumblrButtonOpen = false; pyroButtonOpen = true; diskButtonOpen = true;
                    // tumblr post toggle
                    function tumblrtoggle() { pyroButtonOpen = true; diskButtonOpen = true;
                        if (tumblrButtonOpen == false) { inactiveall(); idleactive(); tumblrButtonOpen = true;
                            document.getElementById("postwindow").style.backgroundImage = currentinactiveheader
                            document.getElementById("taskapp1").style.backgroundImage = github + "img/Taskbar%20application1.png')";
                            document.getElementById("desktop").style.left = "-20000px"; document.body.style.overflowY = "hidden";
                        } else if (tumblrButtonOpen == true){ inactiveall(); tumblrButtonOpen = false;
                            document.getElementById("post").style.zIndex = (currentlayer = currentlayer + 1); currentlayer = currentlayer + 1;
                            document.getElementById("postwindow").style.backgroundImage = currentactiveheader;
                            document.getElementById("taskapp1").style.backgroundImage = github + "img/Taskbar%20application.png')";
                            document.getElementById("desktop").style.left = "0px"; document.body.style.overflowY = "scroll";}}
                    // random message box active toggle
                    function pyrotoggle() { tumblrButtonOpen = true; diskButtonOpen = true;
                        if (pyroButtonOpen == false) { inactiveall(); idleactive(); pyroButtonOpen = true;
                            document.getElementById("pyrowindow").style.backgroundImage = currentinactiveheader
                            document.getElementById("catwindow").style.backgroundImage = currentinactiveheader
                            document.getElementById("taskapp2").style.backgroundImage = github + "img/Taskbar%20application1.png')";
                        } else if (pyroButtonOpen == true){ inactiveall(); pyroButtonOpen = false;
                            if (window1 == true){ 
                                document.getElementById("pyro").style.zIndex = (currentlayer = currentlayer + 1); currentlayer = currentlayer + 1;
                                document.getElementById("pyrowindow").style.backgroundImage = currentactiveheader;  
                                document.getElementById("taskapp2").style.backgroundImage = github + "img/Taskbar%20application.png')"; 
                            } else if (window2 == true){ 
                                document.getElementById("cat").style.zIndex = (currentlayer = currentlayer + 1); currentlayer = currentlayer + 1;
                                document.getElementById("catwindow").style.backgroundImage = currentactiveheader;  
                                document.getElementById("taskapp2").style.backgroundImage = github + "img/Taskbar%20application.png')";}}}
                    // disk window active toggle
                    function disktoggle() { tumblrButtonOpen = true; pyroButtonOpen = true;
                        if (diskButtonOpen == false) { inactiveall(); idleactive(); diskButtonOpen = true;
                            document.getElementById("diskwindow").style.backgroundImage = currentinactiveheader
                            document.getElementById("taskapp3").style.backgroundImage = github + "img/Taskbar%20application1.png')";
                            document.getElementById("disk").style.marginTop = "-20000px";
                        } else if (diskButtonOpen == true){
                            inactiveall(); diskButtonOpen = false;
                            document.getElementById("disk").style.zIndex = (currentlayer = currentlayer + 1); currentlayer = currentlayer + 1;
                            document.getElementById("disk").style.marginTop = "0px";
                            document.getElementById("diskwindow").style.backgroundImage = currentactiveheader;  
                            document.getElementById("taskapp3").style.backgroundImage = github + "img/Taskbar%20application.png')";}} 
                    function idleactive() { tumblrButtonOpen = true; pyroButtonOpen = true; diskButtonOpen = true; }
                </script>
                 
                <div class="clock">
                    <taskbar class="calendar" style="cursor: pointer;" onclick="clockopen();"> </taskbar>

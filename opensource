        var discord = "url('https://cdn.discordapp.com/attachments/";
        
        $(document).ready(function(){ resizeVideos() });
        function resizeVideos() {
           $('.video iframe:not(.tumblr_video_iframe)').each(function(){
               var scale = $(this).parents('.video').width() / 500;
               $(this).attr({ width: Math.floor($(this).attr('width') * scale),
                              height: Math.floor($(this).attr('height') * scale) })});
           $('.tumblr_video_iframe').each(function(){
               var scale = $(this).parents('.video').width() / 500;
               $(this).css({ 'transform': 'scale(' + scale + ')',
                             'margin': 250 * ( scale - 1 ) + 'px' });
               $(this).parents('.tumblr_video_container').css({ width: Math.floor($(this).width() * ( scale + 1 ) / 2),
                                                                height: Math.floor($(this).height() * ( scale + 1 ) / 2)})})};
    </script>
    

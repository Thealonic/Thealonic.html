            <script>   
                //start button / menu addressing
                let startButtonOpen = false; 
                function startpress() { if (startButtonOpen == true) { document.getElementById("startmenu").style.marginLeft = "200000px"; startButtonOpen = false;
                    } else if(startButtonOpen == false) { document.getElementById("startmenu").style.marginLeft = "0px"; startButtonOpen = true; }}
                //start button pressed // start menu window
                function menuclosed(){ document.getElementById("startmenu").style.marginLeft = "200000px"; startButtonOpen = false; }
            </script> 
            
            <!-- Start button toggle -->
            <div class="startbutton" id="startbutton" onclick="startpress()"> </div> 
            

                <!-- taskbar application icons -->
                <taskbar class="big_seperator" style="margin-left:58px"> </taskbar>
                <taskbar class="small_seperator" style="cursor:e-resize"> </taskbar>
                <div class="taskbaricons">
                    <div class="taskbaricon1"></div>

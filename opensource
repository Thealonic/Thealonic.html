        <script>
            function logonactive(){ idleactive(); inactiveall();
                document.getElementById("logoff").style.zIndex = (currentlayer = currentlayer + 1); currentlayer = currentlayer + 1;
                document.getElementById("logoffwindow").style.backgroundImage = currentactiveheader; }
            function logoff() {   document.getElementById("logoff").style.marginLeft = "0px"; document.getElementById("logoff").style.zIndex = "100"; }
            function logoffno() { document.getElementById("logoff").style.marginLeft = "200000px"; document.getElementById("logoff").style.zIndex = "1"; }
        </script>


        <!-- ----------------------------------------------------------------------------------------------------------------------------- -->
        <!-- On page load handling                                                                                                         -->
        <!-- ----------------------------------------------------------------------------------------------------------------------------- -->
        
        <script>

            $(document).ready(function(){
                var infScr = $('body').hasClass('infinite_scrolling');
                var twoCol = $('body').hasClass('two_column');
                var $container = $('#desktop');

                    //two coloumn loading 
                if( infScr && twoCol ){
                    $container.imagesLoaded(function(){ $container.masonry({ itemSelector: '.window'}) });
                    $container.infinitescroll({ itemSelector: '.window'},
                        function( newElements ) {
                            var $newElems = $( newElements ).css({ opacity: 1 }); 
                            var $newElemsIDs = $newElems.map(function () { 
                                return this.id; 
                            }).get();
                            Tumblr.LikeButton.get_status_by_post_ids($newElemsIDs);
                            $newElems.imagesLoaded(function(){ $container.masonry( 'appended', $newElems, true )})
                            $newElems.find('.video').each( function() { resizeVideos() });
                })}
            
                //taskbar clock functions
                function updateClock(){
                    var date = new Date();
                    var mins = date.getMinutes(); var hrs = date.getHours(); var ext = "AM";
                    if(hrs > 12){ var ext = "PM"; var hrs = hrs - parseInt(12); }
                    if(hrs < 10){ var hrs = "0"+hrs; }
                    if(mins < 10){ var mins = "0"+mins; }
                    var currentTime = hrs + ":" + mins + " " + ext;
                    $('#showClock').html(currentTime); }
                window.setInterval(updateClock, 6000);
            
            updateClock()});
            
        // ----------------------------------------------------------------------------------------------------------------------------- -->
        // Drag element scripts                                                                                                          -->
        // ----------------------------------------------------------------------------------------------------------------------------- -->
            
                //drag element id grabber
                dragElement(document.getElementById("cat"));
                dragElement(document.getElementById("pyro"));
                dragElement(document.getElementById("disk"));
                dragElement(document.getElementById("message"));
                dragElement(document.getElementById("logoff"));
                dragElement(document.getElementById("shutdown"));
                
                //grab element for windows
                function dragElement(elmnt) { //position storage and setting it to window
                var pos1 = 0, pos2 = 0, pos3 = 0, pos4 = 0;
                if (document.getElementById(elmnt.id + "window")) { document.getElementById(elmnt.id + "window").onmousedown = dragMouseDown; } else { elmnt.onmousedown = dragMouseDown; }
                function dragMouseDown(e) { //mouse moment grabbing
                    e = e || window.event; e.preventDefault(); pos3 = e.clientX; pos4 = e.clientY;
                    document.onmouseup = closeDragElement; document.onmousemove = elementDrag; }
                function elementDrag(e) { e = e || window.event;  e.preventDefault();
                   pos1 = pos3 - e.clientX; pos2 = pos4 - e.clientY; pos3 = e.clientX; pos4 = e.clientY;
                    elmnt.style.top = (elmnt.offsetTop - pos2) + "px"; elmnt.style.left = (elmnt.offsetLeft - pos1) + "px"; }
                function closeDragElement() { document.onmouseup = null; document.onmousemove = null; }}
        
